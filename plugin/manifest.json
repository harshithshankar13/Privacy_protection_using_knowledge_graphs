{
    "name": "Privacy protection",
    "description" : "user data privary extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "pp.png"
    },
    "permissions":["<all_urls>", "tabs", "http://localhost:5000/","webRequest", "geolocation"],
    "background":{
      "scripts":["jquery.js","background.js"]
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://connect.facebook.net/en_US/sdk.js 'sha256-4RMM1+muFrUXcDP1ofmFpeg5ocWvYLZ8lUqmgdf+bvg='; object-src 'self'"
}