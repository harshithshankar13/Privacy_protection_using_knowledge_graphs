{
    "name": "Privacy protection",
    "description" : "user data privary extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "assets/pp.png"
    },
    "permissions":["<all_urls>", "tabs", "http://localhost:5000/","webRequest", "geolocation", "history", "storage"],
    "background":{
      "scripts":["jquery.js","background.js", "getContent.js"]
    },
    "content_scripts": [ {
      "matches": ["<all_urls>"],
      "js": ["getContent.js"],
      "run_at": "document_end"
    } ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com 'sha256-xI5baLkftKyKzTe/X2jh+r6IsMvAqLo73D0dmyZPs3M='; object-src 'self'"
}